<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="travelingViajes.Views.InicioSesionAdmin" Padding="10">
    <ContentPage.Content>
        <StackLayout>
            <Frame BackgroundColor="blue" CornerRadius="10" HorizontalOptions="Center">
                <Label Text="Inicio de Sesión" TextColor="White"/>
            </Frame>
            <StackLayout>
                <Label Text="Usuario:"/>
                <Entry Placeholder="Ingrese su Usuario" x:Name="EntryUsuario"/>
                <Label Text="Contraseña:"/>
                <Entry Placeholder="Ingarese su contraseña" x:Name="EntryContasena"/>
                <Button Text="Iniciar Sesión" x:Name="btnIniciar" Clicked="btnIniciar_Clicked"/>
                <Button Text="Registrarse" x:Name="btnRegistro" Clicked="btnRegistro_Clicked"></Button>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>